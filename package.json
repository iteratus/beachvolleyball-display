{
  "name": "beachvolleyball-display",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "style": "npx stylelint **/*.scss --fix",
    "format": "prettier --write \"./pages/**/*.{js,json}\" \"./*.{js,json}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
